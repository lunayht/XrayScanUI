{
	"name": "rapiscan",
	"version": "1.0.0",
	"description": "UI design for xray machine console and video stream ",
	"main": "main.js",
	"scripts": {
		"start": "nodemon --exec babel-node server/app.js",
		"start:prod": "babel-node server/app.js",
		"dev": "rimraf dist && npm run webpack:dev && npm run start",
		"build": "rimraf dist && npm run webpack:prod && npm run start:prod",
		"webpack:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.dev.js",
		"webpack:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
		"migrate": "knex migrate:latest",
		"clean": "rimraf node_modules && rimraf dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tanyinghui/XrayScanUI.git"
	},
	"keywords": [
		"reactjs"
	],
	"author": "Ying Hui",
	"license": "ISC",
	"homepage": "https://github.com/tanyinghui/XrayScanUI#readme",
	"dependencies": {
